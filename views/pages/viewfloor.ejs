<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title></title>
	<link rel="stylesheet" href="/stylesheets/viewfloor.css">
</head>
<body>
	<div class="header">
    	<h1>Nurse's Home</h1>
    </div>
    <div class="content">
    <table>
    	<tr>
    		<th>Patient ID</th>
    		<th>Name</th>
    		<th>Room</th>
    		<th>Doctor</th>
            <th>Department</th>
    	</tr>
    	<% for(var i=0;i<data.length;i++){ %>
		    <tr>
		    	<td><%= data[i].patient_id %></td>
		    	<td><%= data[i].patient_name %></td>
		    	<td><%= data[i].room %></td>
		    	<td><%= data[i].doctor_name %></td>
                <td><%= data[i].dept %></td>
		    </tr>
	    <% } %>
    </table>
    </div>
</body>
</html>